{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Feature-Policy",
          "value": "*"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/FG_Virgil.woff2",
      "destination": "/static/fonts/FG_Virgil.woff2"
    },
    {
      "source": "/Cascadia.woff2",
      "destination": "/static/fonts/Cascadia.woff2"
    },
    {
      "source": "/([^.]+)",
      "destination": "/",
      "statusCode": 301
    }
  ]
}
