{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Feature-Policy",
          "value": "*"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/([^.]+)",
      "destination": "/",
      "statusCode": 301
    }
  ]
}
